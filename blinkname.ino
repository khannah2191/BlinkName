void setup() {
    pinMode(D7, OUTPUT);
}

void loop() {
    // blink letter N
    digitalWrite(D7, HIGH);
    delay(600);
    digitalWrite(D7, LOW);
    delay(300);
    digitalWrite(D7, HIGH);
    delay(300);
    digitalWrite(D7, LOW);
    delay(300);
    
    delay(800);
    
    // blink letter A
    digitalWrite(D7, HIGH);
    delay(300);
    digitalWrite(D7, LOW);
    delay(300);
    digitalWrite(D7, HIGH);
    delay(600);
    digitalWrite(D7, LOW);
    
    delay(800);
    
    // blink letter H
    digitalWrite(D7, HIGH);
    delay(300);
    digitalWrite(D7, LOW);
    delay(300);
    digitalWrite(D7, HIGH);
    delay(300);
    digitalWrite(D7, LOW);
    delay(300);
    digitalWrite(D7, HIGH);
    delay(300);
    digitalWrite(D7, LOW);
    delay(300);
    digitalWrite(D7, HIGH);
    delay(300);
    digitalWrite(D7, LOW);
    delay(300);
    
    delay(800);
    
    // blink letter I
    digitalWrite(D7, HIGH);
    delay(300);
    digitalWrite(D7, LOW);
    delay(300);
    digitalWrite(D7, HIGH);
    delay(300);
    digitalWrite(D7, LOW);
    delay(300);
    
    delay(800);
    
    // blink letter D
    digitalWrite(D7, HIGH);
    delay(600);
    digitalWrite(D7, LOW);
    delay(300);
    digitalWrite(D7, HIGH);
    delay(300);
    digitalWrite(D7, LOW);
    delay(300);
    digitalWrite(D7, HIGH);
    delay(300);
    digitalWrite(D7, LOW);
    delay(300);

    delay(1300);

}